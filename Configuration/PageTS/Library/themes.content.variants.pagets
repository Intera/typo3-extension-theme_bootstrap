themes {
	content {
		# Variants for content elements
		variants {
			default {
				alert-success = Alert-Success
				alert-info = Alert-Info
				alert-warning = Alert-Warning
				alert-danger = Alert-Danger
				showcase = Showcase
				framed-box = Framed box
				framed-box-small = Framed box (small)
				framed-box-large = Framed box (large)
			}
			# Special variant for Header
			# Used by ctype header
			# Will be merged with default
			header {
				page-header = Page-Header
			}
		}
	}
}